/*******************************************************************************
 * Obstacle Map
 */
define(['./constants', './obstacleFactory'], function(Constants, ObstacleFactory){
    function ObstacleMap(){
        this.arr = [];
        var newWall;
        for(var i = 0; i < Constants.canvasHeight + Constants.gridSize; i+=Constants.gridSize){
            newWall = [];
            for(var j = 0; j < Constants.canvasWidth + Constants.gridSize; j+=Constants.gridSize){
                newWall.push(obsFactory.getRandomObs(j, i));
            }
            this.arr.push(newWall);
        }
    }
});
